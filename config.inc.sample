VERSION=20250720
RELEASE=6.1.0
DESTDIR=/DRIVE/minc-release
GCCVER=4.8.1
MINGW=/C/MinGW
SRCDIR=/D/openbsd-master
INSTALL=install
ARCH=i386

# locations for native compiler

PREFIX=/opt
BINDIR=${PREFIX}/bin
LIBDIR=${PREFIX}/lib
INCDIR=${PREFIX}/include
GCCLIB=${BINDIR}/libisl-15.dll
GCCLIB+=${BINDIR}/libmingwex-0.dll

# locations for cross-compiler

XPREFIX=/usr/i686-openbsd-pe
XLIBDIR=${XPREFIX}/lib
XINCDIR=${XPREFIX}/include

# location of packages (release.sh)

PKGROOT=/mnt/d/src
